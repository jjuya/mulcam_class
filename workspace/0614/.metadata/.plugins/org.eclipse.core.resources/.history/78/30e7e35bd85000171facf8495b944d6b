package basic.exam2;

public class ForTest {
	public static void main(String[] args) {

		int sum = 0;
		
		for(int i = 1; i <= 3; i++) {
			sum += i;
		} //end for
		
		System.out.println(sum);
		
		int sum3 = 0;
		
		for(int i = 3; i <= 1000; i += 3) {
			sum += i;
		} //end for
		
		System.out.println(sum3);
		
	} //end maim
} //end class
