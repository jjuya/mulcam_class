package basic.exam2;

public class WhileTest {
	public static void main(String[] args) {
		
//		while(Á¶°Ç½Ä) {
//			
//		}
		
		int i = 1;
		
		while(i <= 10) {
			System.out.print((i*10) + " : ");
			System.out.println("Hi");
			
			i++;
		}
		
		System.out.println("End");
		
		int sum = 0;
		int j = 0;
		
		while(j <= 1000) {
			j = j + 3;
			
			sum = j + sum;
		}
		
		System.out.println(sum);
	} //end main
} //end class
