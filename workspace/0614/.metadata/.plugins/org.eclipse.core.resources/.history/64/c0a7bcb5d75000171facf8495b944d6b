package basic.exam2;

public class ForTest {
	public static void main(String[] args) {

//		for(초기식; 조건식; 증감식) {
//			
//		}
		
		
		for(int i = 1; i <= 10; i++) {
			System.out.print(i * 10 + " : ");
			System.out.println("Hi");
		} //end for
		
		System.out.println("End");
		
	} //end maim
} //end class
