package basic.exam2;

public class WhileTest {
	public static void main(String[] args) {
		
//		while(Á¶°Ç½Ä) {
//			
//		}
		
		int i = 1;
		
		while(i <= 10) {
			System.out.print((i*10) + " : ");
			System.out.println("Hi");
			
			i++;
		}
		
		System.out.println("End");
		
	} //end main
} //end class
