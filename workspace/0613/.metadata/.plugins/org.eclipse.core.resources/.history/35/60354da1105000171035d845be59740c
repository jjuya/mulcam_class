package basic.exam2;

import java.util.Scanner;

public class IfTest {
	public static void main(String[] args) {
		
		int n = 1;
		
		if(n == 1) {
			System.out.println("Man");
		}
		else if(n == 2) {
			System.out.println("Waman");
		}
		else {
			System.out.println("Error");
		}
		
		System.out.println("End");
		
		
	Scanner sc = new Scanner(System.in)
	}
}
