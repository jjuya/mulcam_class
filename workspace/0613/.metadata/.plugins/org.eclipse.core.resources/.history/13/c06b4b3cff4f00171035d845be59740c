package basic.exam;

public class Arithmatic2Test {
	public static void main(String[] args) {
		int i;
		int j;
		
		System.out.print("i : ");
		i = sc.nextInt();
		
		System.out.print("j : ");
		j = sc.nextInt();
		
		System.out.println(i > j);
		System.out.println(i < j);
		System.out.println(i >= j);
		System.out.println(i >= j);
		System.out.println(i == j);
		System.out.println(i != j);
	}
}
