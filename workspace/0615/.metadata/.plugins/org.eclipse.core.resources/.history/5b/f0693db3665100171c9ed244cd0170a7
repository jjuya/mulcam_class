package basic.exam2;

public class BreakTest {
	public static void main(String[] args) {
		
		int sum = 0;
		int i = 1
				
		for(; sum < 30; i++) {
			sum += i;
		} //end for
		
		System.out.println(sum);
		
		sum = 0;
		
		for(int i = 1; sum < 30; i++) {
			sum += i;
			
			if(sum >= 30) break;
		} //end for

		System.out.println(sum);
		
	} //end main
} //end class
