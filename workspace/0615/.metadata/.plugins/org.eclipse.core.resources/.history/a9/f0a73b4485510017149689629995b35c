package basic.exam2;

public class ForTest4 {
	public static void main(String[] args) {

		for(int i = 1; i <= 9; i++) {
			for(int j = 2; j <= 9; j++) {
				System.out.print(j + " * "  + i + " = " + (i * j) + "\t");
			} //end inner for

			System.out.println();
		} //end outer for

		System.out.println();


		for(int i = 1; i <= 2; i++) {
			for(int j = 2; j <= 5; j++) {
				System.out.print(j + " * "  + i + " = " + (i * j) + "\t");
			} //end inner for

			System.out.println();

			for(int k = 6; k <= 9; k++) {
				System.out.print(k + " * "  + i + " = " + (i * k) + "\t");
			} //end inner for

			System.out.println();
		} //end outer for

		System.out.println();
		
		// 도형오빠
		for(int row = 0; row < 2; row++) {
			for(int i = 1; i <=9 ; i++) {
				for(int j = 2 + (4 * row); j <= 5 +(4 * row); j++) {
					System.out.print(j + " * "  + i + " = " + (i * j) + "\t");
				}
				System.out.println();
			}

			System.out.println();
		}

		System.out.println();
		
		//희남오빠
		for (int i = 0; i <= 4; i+=4) {
			for (int j = 1; j <= 9; j++) {
				for (int k = 2; k <= 5; k++) {
					System.out.print((k+i) + "x" + j + "=" + (k+i) * j + "\t");
					}
				System.out.println();
		
			}
			System.out.println();

		}

	} //end main
} //end class
