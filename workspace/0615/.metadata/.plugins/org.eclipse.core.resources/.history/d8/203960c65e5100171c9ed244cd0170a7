package basic.exam2;

public class DoWhileTest {
	public static void main(String[] args) {
		 
		for(int i = 1, j = 1; i <= 10; i++) {
			System.out.println((i * 10) + " " + j + " : Ho");
		} //end for
		
		int i = 1;
		
		do {
			System.out.println("Ho");
			i++
		} while(i <= 3);
		
	} //end main
} //end main
