package basic.exam2;

public class DoWhileTest2 {
	public static void main(String[] args) {
		 
		
//		int i = 1;
//		
//		do {
//			System.out.println("Ho");
//			i++;
//		} while(i <= 3);
		
		int x, y;
		
		x = (int)(Math.random() * 6) + 1;
		
		do {
			y = (int) (Math.random() * 6) + 1;
		} while (x == y);
		
		System.out.println(x);
		System.out.println(y);
		
	} //end main
} //end main
