package basic.exam2;

public class ForTest4 {
	public static void main(String[] args) {

		for(int i = 1; i <= 9; i++) {
			for(int j = 2; j <= 9; j++) {
				System.out.print(j + " * "  + i + " = " + (i * j) + "\t");
			} //end inner for

			System.out.println();
		} //end outer for

		System.out.println();


		for(int i = 1; i <= 9; i++) {
			for(int j = 2; j <= 5; j++) {
				System.out.print(i + " * "  + j + " = " + (i * j) + "\t");
			} //end inner for
			
			System.out.println();
			
			for(int k = 6; k <= 9; k++) {
				System.out.print(k + " * "  + i + " = " + (i * k) + "\t");
			} //end inner for
			
			System.out.println();
		} //end outer for


	} //end main
} //end class
